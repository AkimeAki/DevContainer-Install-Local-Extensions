{
	"$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
	"vcs": {
		"enabled": false,
		"clientKind": "git",
		"useIgnoreFile": false
	},
	"files": {
		"ignoreUnknown": false,
		"ignore": ["dist/**/*", "node_modules/**/*", ".git/**/*"]
	},
	"formatter": {
		"enabled": false
	},
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true
	}
}
